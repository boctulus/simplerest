{
	"name":"boctulus/simplerest",
   "type":"project",
   "description":"API Rest framework for PHP",
   "keywords":[
      "simplerest",
      "framework",
      "boctulus"
   ],
   "license":"MIT",
   "authors":[
      {
         "name":"Pablo Gabriel Bozzolo",
         "email":"boctulus@gmail.com"
      }
   ],
   "version":"0.8.12",
   "require":{
      "php":">=7.4,<8.4",      
      "vlucas/phpdotenv": "^5.2",
      "simplerest/shopifyconnector": "@dev",
		"nikic/php-parser": "^5.4",
		"doctrine/inflector": "^2.0",
      "kreait/firebase-php": "^7.0",
		"google/cloud-firestore": "^1.50",
		"google/protobuf": "^4.30"
   },
   "scripts":{
      "pre-autoload-dump":"Google\\Task\\Composer::cleanup"
   },
   "extra":{
      "google/apiclient-services":[
         "Drive"
      ]
   },
   "autoload":{
      "psr-4":{
         "Boctulus\\Simplerest\\":"app/",
         "Boctulus\\Simplerest\\FineGrainedACL\\":"packages/simplerest/fine-grained-acl/src",
         "Boctulus\\Simplerest\\BasicACL\\":"packages/simplerest/basic-acl/src",
         "Boctulus\\Zippy\\":"packages/boctulus/zippy/src",
         "Boctulus\\CliTest\\":"packages/boctulus/cli-test/src"
      }
   },
   "repositories": [    
    {
        "type": "path",
        "url": "packages/simplerest/shopifyconnector",
        "options": {
            "symlink": true
        }
    }
   ],
   "config":{
      "optimize-autoloader":true,
      "allow-plugins":{
         "php-http/discovery":true
      },      
      "preferred-install": {
        "boctulus/api-client": "dist",
        "boctulus/shopifyconnector": "dist"
      }
   },
   "minimum-stability":"stable",
   "prefer-stable":true,
	"require-dev": {
		"phpunit/phpunit": "10"
	}
}
