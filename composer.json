{
	"name":"boctulus/simplerest",
   "type":"project",
   "description":"API Rest framework for PHP",
   "keywords":[
      "simplerest",
      "framework",
      "boctulus"
   ],
   "license":"MIT",
   "authors":[
      {
         "name":"Pablo Gabriel Bozzolo",
         "email":"boctulus@gmail.com"
      }
   ],
   "version":"0.8.12",
   "require":{
      "php":">=7.4",
      "firebase/php-jwt": "^6.0",
      "vlucas/phpdotenv": "^5.2",
      "boctulus/api-client": "@dev",
      "boctulus/shopifyconnector": "@dev",
		"nikic/php-parser": "^5.4",
		"doctrine/inflector": "^2.0"
   },
   "scripts":{
      "pre-autoload-dump":"Google\\Task\\Composer::cleanup"
   },
   "extra":{
      "google/apiclient-services":[
         "Drive"
      ]
   },
   "autoload":{
      "psr-4":{
         "simplerest\\":"app/",
         "devdojo\\calculator\\":"packages/devdojo/calculator/src",
         "boctulus\\grained_acl\\":"packages/boctulus/grained_acl/src",
         "boctulus\\basic_acl\\":"packages/boctulus/basic_acl/src",
         "boctulus\\hello_world\\":"vendor/boctulus/hello_world/src"         
      }
   },
   "repositories": [
    {
        "type": "path",
        "url": "packages/boctulus/api-client",
        "options": {
            "symlink": true
        }
    },
    {
        "type": "path",
        "url": "packages/boctulus/shopifyconnector",
        "options": {
            "symlink": true
        }
    }
   ],
   "config":{
      "optimize-autoloader":true,
      "allow-plugins":{
         "php-http/discovery":true
      },
      "platform":{
         "php":"7.4"
      },
      "preferred-install": {
        "boctulus/api-client": "dist",
        "boctulus/shopifyconnector": "dist"
      }
   },
   "minimum-stability":"stable",
   "prefer-stable":true,
	"require-dev": {
		"phpunit/phpunit": "9"
	}
}
